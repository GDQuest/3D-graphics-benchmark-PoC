; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Node",
"class": &"QualitySettingsBenchmark",
"language": &"GDScript",
"path": "res://QualitySettingsBenchmarker/Benchmark.gd"
}, {
"base": "Resource",
"class": &"QualitySettingsResource",
"language": &"GDScript",
"path": "res://QualitySettingsResource/quality_settings_resource.gd"
}]
_global_script_class_icons={
"QualitySettingsBenchmark": "",
"QualitySettingsResource": ""
}

[application]

config/name="3D Graphics Benchmark PoC"
run/main_scene="res://control.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[display]

window/dpi/allow_hidpi=false
window/ios/allow_high_refresh_rate=false
window/ios/hide_home_indicator=false
window/ios/hide_status_bar=false
window/ios/suppress_ui_gesture=false
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"

[rendering]

anti_aliasing/quality/use_debanding=true
